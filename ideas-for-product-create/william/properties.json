[
    {
        "id": "4e56ef1d-502b-491f-a90e-8303d11d2b55",
        "model": "game",
        "type": "game-selector",
        "label": "properties.default_label",

        "pagination": {
            "table": "games",
            "filters": {
                "enabled": true
            }
        },
        "props": {
            "label": "properties.default_label"
        },
        "validations": ["yup.string", "yup.required"]
    },
    {
        "type": "select",

        "props": {
            "label": "properties.default_label"
        },
        "validations": ["yup.string", "yup.required"]
    },
    {
        "id": "9b656422-8ab0-470b-983d-e76deba693b3",
        "model": "title",
        "type": "input-text",
        "props": {
            "label": "properties.default_label"
        },
        "validations": [
            "yup.string",
            "yup.required",
            ["yup.min", 5, "wizard.validations.step2.title.minLength"],
            ["yup.max", 1500, "wizard.validations.step2.description.maxLength"],
            ["overgear.textRation", "wizard.validations.step2.title.textRatio"]
        ]
    },
    {
        "id": "58e96a91-9985-4073-9add-ca6b911c3636",
        "model": "description",
        "type": "input-text-area",

        "props": {
            "label": "properties.default_label",
            "placeholder": "placeholder.product_description"
        },
        "validations": [
            "yup.string",
            "yup.required",
            ["yup.min", 5, "wizard.validations.step2.title.minLength"],
            ["yup.max", 1500, "wizard.validations.step2.description.maxLength"]
        ]
    },
    {
        "type": "read-only-title",
        "props": { "label": "properties.default_label" }
    },
    {
        "type": "read-only-title-sub",
        "props": { "label": "properties.default_label" }
    },
    {
        "type": "input-duration",
        "id": "6e8614e1-487f-4b41-ab23-27842b1ac4fe",
        "model": "options.duration",
        "validations": ["yup.number", "yup.required", "overgear.isFuture"],
        "props": { "label": "properties.default_label" }
    },
    {
        "type": "input-currency",
        "id": "c7d607ee-f676-4fc7-90d0-ef09479f36ba",
        "model": "options.basePrice",
        "validations": ["yup.number", "yup.required", "yup.min", 0.1],
        "props": {
            "showCommission": true,
            "label": "properties.default_label",
            "tooltip": {
                "type": "info-tooltip",
                "label": "commission.will.apply"
            }
        },
        "showIf": {
            "path": "options.emptyPrice",
            "equals": false
        }
    },
    {
        "id": "f7a8658a-e756-4d69-89ee-247ff4709411",
        "model": "checkbox",
        "type": "input-checkbox",
        "props": { "label": "properties.default_label" },
        "validations": [["oneOf", [false, true]]]
    },
    {
        "id": "01c40431-bfc6-4a84-8eb2-78245c1750ba",
        "type": "input-additional-options",
        "validations": [
            ["yup.arrayOf"],
            ["yup.object"],
            [
                "yup.shape",
                {
                    "title": [
                        ["yup.object"],
                        [
                            "yup.shape",
                            {
                                "en": [
                                    "yu.string",
                                    "yup.required",
                                    ["yup.min", 5, "String must be at least 5 characters"],
                                    ["yup.max", 50, "String must be at most 50 characters"]
                                ],
                                "ru": [
                                    "yu.string",
                                    "yup.required",
                                    ["yup.min", 5, "String must be at least 5 characters"],
                                    ["yup.max", 50, "String must be at most 50 characters"]
                                ]
                            }
                        ]
                    ],
                    "value": [["yup.number"], ["yup.required"], ["yup.min", 5]]
                }
            ]
        ],
        "props": {
            "maxOptions": 25
        }
    }
]
